<p style="color: green"><%= notice %></p>

<h1>Pokers</h1>

<div id="pokers">
  <% @pokers.each do |poker| %>
    <%= render poker %>
    <p>
      <%= link_to "Show this poker", game_room_poker_path(@game_room, poker) %>
    </p>
  <% end %>
</div>

<%= link_to "New poker", new_game_room_poker_path(@game_room) %>
